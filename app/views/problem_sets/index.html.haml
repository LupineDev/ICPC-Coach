%h1 Listing problem_sets

%table
  %tr
    %th Name

- for problem_set in @problem_sets
  %tr
      %td=h problem_set.name
  
    %td= link_to 'Show', problem_set_path(problem_set)
    %td= link_to 'Edit', edit_problem_set_path(problem_set)
    %td= link_to 'Destroy', problem_set_path(problem_set), :confirm => 'Are you sure?', :method => :delete, :confirm => "Are you sure?", :method => :delete
    
= link_to 'New problem_set', new_problem_set_path 